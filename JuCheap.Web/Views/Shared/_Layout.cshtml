@using StackExchange.Profiling
@model List<JuCheap.Models.MenuDto>
@{
    Layout = null;
}

<!DOCTYPE html>
<html>


<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <title>TOM - 主页</title>

    <meta name="keywords" content="JuCheap后台主题,后台bootstrap框架,会员中心主题,后台HTML,响应式后台管理系统">
    <meta name="description" content="JuCheap是一个完全响应式，基于Bootstrap3开发的扁平化主题，采用了主流的左右两栏式布局，使用了Html5+CSS3等现代技术搭建的一个快速开发后台管理系统MVC框架">

    <!--[if lt IE 9]>
    <meta http-equiv="refresh" content="0;ie.html" />
    <![endif]-->
    @Styles.Render("~/Content/css/main", "~/content/css/plugins/layer")
    @Html.Partial("BaiduTongji")
</head>

<body class="fixed-sidebar full-height-layout gray-bg" style="overflow:hidden">
    <div id="wrapper">
        <!--左侧导航开始-->
        @Html.Partial("Menu")
        <!--左侧导航结束-->
        <!--右侧部分开始-->
        @RenderBody()
        @MiniProfiler.RenderIncludes()
        <!--右侧部分结束-->
    </div>
    @Scripts.Render("~/content/js/main")
    <script src="~/Content/js/plugins/layer/layer.js"></script>
</body>
</html>
